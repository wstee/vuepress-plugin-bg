{
  "name": "vuepress-plugin-bg",
  "version": "2.0.0-rc.2",
  "description": "Background Plugin for vuepress theme",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "type": "module",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "pnpm build:tsc & pnpm run copy",
    "build:tsc": "tsc -b tsconfig.build.json",
